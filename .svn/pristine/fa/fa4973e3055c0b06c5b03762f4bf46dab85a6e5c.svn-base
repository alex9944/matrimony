}
.ss_webhosting a {
    font-family: Arial, Helvetica, sans-serif;
    font-size: 13px;
    color: #252525;
    text-decoration: none;
    font-weight: bold;
}
.ss_webhosting tr.noborders td {
    border-bottom: solid 0px;
    border-top: solid 0px;
}
.ss_webhosting tr.bestprices td {
}
.ss_webhosting tr.bestprices {
    border: solid 2px #729341;
}
.ss_webhosting tr.noborders strike {
    font-size: 16px;
    color: #ffffff;
}
.ss_webhosting strike {
    font-size: 10px;
    color: #454545;
}
.status_yes {
	background-image: url(../images/icons/accept_16_payment.png);
	background-repeat: no-repeat;
	display: block;
	height: 16px;
	width: 16px;
	margin-right: auto;
	margin-left: auto;
	background-position: left top;
	font-size: 11px;
	line-height: 11px;
}
.status_no {
	background-image: url(../images/icons/remove__16_payment.png);
	background-repeat: no-repeat;
	display: block;
	height: 16px;
	width: 16px;
	margin-right: auto;
	margin-left: auto;
	background-position: left top;
	font-size: 11px;
	line-height: 11px;
}

table.tbl_cart,table.tbl_cart td, table.tbl_cart th
{
border-collapse:collapse;
}

table.tbl_cart tr th
{
	color: #A50050;
	background-color: #FCECD6;
	padding-top: 10px;
	padding-bottom: 10px;
	padding-left: 5px;
	padding-right: 5px;
	font-weight: bold;
	font-size: 14px;
	border-top-width: 1px;
	border-right-width: 1px;
	border-bottom-width: 2px;
	border-left-width: 1px;
	border-top-style: solid;
	border-right-style: solid;
	border-bottom-style: solid;
	border-left-style: solid;
	border-top-color: #A50050;
	border-right-color: #A50050;
	border-bottom-color: #A50050;
	border-left-color: #A50050;
	
}
table.tbl_cart tr.subsection th
{
	background-color:#ebebeb;
	font-weight:bold;
	color: #A50050;
	border: 1px solid #CCC;
}

table.tbl_cart tr td
{
	padding-top: 8px;
	padding-right: 5px;
	padding-bottom: 8px;
	padding-left: 5px;
	border: 1px dotted #A50050;
}

table.tbl_cart tr.footer th
{
	color: #A50050;
	background-color: #FCECD6;
	padding-top: 10px;
	padding-bottom: 10px;
	padding-left: 5px;
	padding-right: 5px;
	font-weight: bold;
	border-top-width: 2px;
	border-top-style: solid;
	border-top-color: #A50050;
	text-align: right;
	font-size: 13px;
	
}


