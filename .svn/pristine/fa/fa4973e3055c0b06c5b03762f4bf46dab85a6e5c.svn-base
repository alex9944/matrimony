}
.ss_webhosting tr.alt .plans5 {
	background-color: #C7D5B3;
}
.ss_webhosting .bestvalue {
    background-image: url(../../../images/yootheme/bestvalue_icons.png);
    background-position: right top;
    background-repeat: no-repeat;
}
.ss_webhosting tr.alt .bestvalue {
    background-image: url(../../../images/yootheme/bestvalue_icons.png);
    background-position: right top;
    background-repeat: no-repeat;
}
.ss_webhosting .plans1 small {
    font-size: 10px;
    color: #1257d4;
}
.ss_webhosting .plans2 small {
