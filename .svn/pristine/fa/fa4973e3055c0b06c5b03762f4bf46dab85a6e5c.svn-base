.ss_webhosting .features {
    text-align: left;
    background-color: #ffffff;
}
.ss_webhosting .plans1 {
	background-color: #ECEEF0;
}
.ss_webhosting .plans2 {
	background-color: #FFF1DD;
}
.ss_webhosting .plans4 {
	background-color: #FFF0F8;
}
.ss_webhosting .plans5 {
	background-color: #ECF4D7;
}
.ss_webhosting tr.alt .features {
	text-align: left;
	background-color: #FCECD6;
}
.ss_webhosting tr.alt .plans1 {
	background-color: #C6CBD2;
}
.ss_webhosting tr.alt .plans2 {
	background-color: #FFDFAE;
}
.ss_webhosting tr.alt .plans4 {
