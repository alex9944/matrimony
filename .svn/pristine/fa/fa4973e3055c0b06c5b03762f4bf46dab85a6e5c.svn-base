@charset "utf-8";
/* CSS Document */

.ss_webhosting {
	border-collapse: collapse;
	border: 1px solid #FFF;
}
.ss_webhosting th {
	font-size: 12px;
	border-collapse: collapse;
	border-left: solid 1px #ffffff;
	border-right: solid 1px #ffffff;
	text-align: center;
	padding: 10px 6px;
}
.ss_webhosting td {
	font-size: 12px;
	border-collapse: collapse;
	border-left: solid 1px #ffffff;
