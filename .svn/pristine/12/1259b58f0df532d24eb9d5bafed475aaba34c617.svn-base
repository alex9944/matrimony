@charset "UTF-8";

/* CSS Document */



/* SMALL BUTTONS */

.button-small {

	color: #ffffff !important;

	font: bold 12px/18px Arial, Helvetica, sans-serif;

	display: inline-block;

	height: 28px;

	margin: 0 1em 1em 15px;

	padding: 3px 5px 5px 5px;

	position: relative;

	text-decoration: none !important;

}

.button-small1 {

	color: #ffffff !important;

	font: bold 12px/18px Arial, Helvetica, sans-serif;

	display: inline-block;

	height: 28px;

	margin: 0 1em 1em 15px;

	padding: 5px;

	position: relative;

	text-decoration: none !important;

}

.gdrop {

    background-color: #F2F2F2;

    font-size: 16px;

    font-weight: bold;

    height: auto;

    margin: 0 auto;

}

.button-small span {

	height: 27px;

	left: -15px;

	position: absolute;

	top: 0;

	width: 15px;

}

.button-small:hover {

	text-decoration: none;

}

/* COLORS */





.rounded-orange {

	background: url(../images/rounded-buttons-right.png) no-repeat right -54px;

	text-shadow: 0 1px 1px #9b5510;

}

.rounded-orange:hover {

