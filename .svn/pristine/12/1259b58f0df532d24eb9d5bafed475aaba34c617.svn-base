/* DDNotification */

.ddnotification {

	border-radius: 10px;

	-moz-border-radius: 10px;

	-webkit-border-radius: 10px;

	color: #ffffff;

	display: block;

	font: normal 12px/18px Arial, Helvetica, sans-serif;

	margin: 1em 0;

	padding: 30px 30px 30px 90px;

	position: relative;

}

.ddnotification span.icon {

	height: 37px;

	left: 30px;

	position: absolute;

	top: 30px;

	width: 36px;

}

.ddnotification span.close {

	background: url(../images/notifications/close.png) no-repeat top left;

	cursor: pointer;

	height: 13px;

	position: absolute;

	right: 10px;

	top: 10px;

	width: 13px;

}

.notification-error {

	background: #c02424 url(../images/notifications/error.gif) repeat-x top left;

	border: 1px solid #a01515;

	text-shadow: 0 1px 1px #6f1111;

}

.notification-error span.icon {

	background: url(../images/notifications/error-icon.png) no-repeat top left;

}

.notification-success {

	background: #6ba623 url(../images/notifications/success.gif) repeat-x top left;

	border: 1px solid #6e8a19;

	text-shadow: 0 1px 1px #2d4103;

}

.notification-success span.icon {

	background: url(../images/notifications/success-icon.png) no-repeat top left;

}

.notification-info {

	background: #1d86af url(../images/notifications/info.gif) repeat-x top left;

	border: 1px solid #317d9b;

	text-shadow: 0 1px 1px #064353;

}

.notification-info span.icon {

	background: url(../images/notifications/info-icon.png) no-repeat top left;

}

.notification-alert {

	background: #e77414 url(../images/notifications/alert.gif) repeat-x top left;

	border: 1px solid #be7125;

	text-shadow: 0 1px 1px #6e3d05;

}

.notification-alert span.icon {

	background: url(../images/notifications/alert-icon.png) no-repeat top left;

}

.notification-standard {

	background: #d7d7d7 url(../images/notifications/standard.gif) repeat-x top left;

	border: 1px solid #bababa;

	text-shadow: 0 1px 1px #ffffff;

	color: #464646 !important;

}

.notification-standard span.icon {

	background: url(../images/notifications/standard-icon.png) no-repeat top left;

}

.notification-standard span.close {

	background: url(../images/notifications/close.png) no-repeat bottom left;

}

/* LISTS */



.ddlist {

	list-style: none;

	padding: 0;

}

.list-BW, .list-arrow, .list-hot, .list-tick, .list-star, .list-clip, .list-black, .list-blue, .list-red, .list-green, .list-yellow, .list-purple, .list-orange, .list-pink, .list-lock {

	list-style: none;

	padding: 0;

}

.list-arrow li {

	background:url(../images/icons/bullet_arrow.png) no-repeat left 7px;

	padding: 2px 0 2px 15px;

}

.list-tick li {

	background:url(../images/icons/bullet-tick.png) no-repeat left 5px;

	padding: 2px 0 2px 20px;

}

.list-yellow li {

	background:url(../images/icons/bullet_yellow.png) no-repeat left 8px;

	padding: 2px 0 2px 15px;

}

.list-orange li {

	background:url(../images/icons/bullet_orange.png) no-repeat left 8px;

	padding: 2px 0 2px 15px;

}

.list-pink li {

	background:url(../images/icons/bullet_pink.png) no-repeat left 8px;

	padding: 2px 0 2px 15px;

}

.list-lock li {

	padding-top: 4px;

	padding-right: 0;

	padding-bottom: 4px;

	padding-left: 20px;

	background-image: url(../images/icons/lock16.png);

	background-repeat: no-repeat;

	background-position: left 8px;

}

.list-arrow2 li {

	padding: 2px 0 2px 20px;

	background-image: url(../images/icons/arrow_black.png);

	background-repeat: no-repeat;

	background-position: left 8px;

}

/* PREFORMATTED STYLE - FOR SHOWING HTML CODE  */

pre, xmp {

	position: relative;

	z-index: 50;

	background: white url(../images/pre_bg.gif) top left repeat;

	border: 1px solid #999;

	color: black;

	display: block;

	font-family: "Courier New", Courier, monospace;

	font-size: 13px;

	line-height: 18px;

	margin: 10px 0 20px;

	overflow: auto;

	padding: 18px 10px 17px;

	overflow-x: scroll;

}

/* WRAPPER - COVERING WITH ROUNDED BORDERS - we use Table(3X3) for WRAPPERS  */

table.wrapper1 {

	width: 100%;

}

table.wrapper1 td.td11, table.wrapper1 td.td12, table.wrapper1 td.td13, table.wrapper1 td.td31, table.wrapper1 td.td32, table.wrapper1 td.td33 {

	background-image: url(../images/UI_elements/wrapper1_h.png);

}

table.wrapper1 td.td21, table.wrapper1 td.td23 {

	background-image: url(../images/UI_elements/wrapper1_v.png);

}

table.wrapper1 td.td11 {

	height: 12px;

	width: 12px;

	background-repeat: no-repeat;

	background-position: 0px 0px;

	line-height: 1px;

	font-size: 1px;

}

table.wrapper1 td.td12 {

	height: 12px;

	background-repeat: repeat-x;

	background-position: 0px -18px;

}

table.wrapper1 td.td13 {

	height: 12px;

	background-repeat: no-repeat;

	background-position: 0px -36px;

	width: 12px;

}

table.wrapper1 td.td21 {

	height: 12px;

	background-repeat: repeat-y;

	background-position: 0px 0px;

	width: 12px;

}

table.wrapper1 td.td23 {

	height: 12px;

	background-repeat: repeat-y;

	background-position: -20px 0px;

	width: 12px;

}

table.wrapper1 td.td31 {

	height: 12px;

	background-repeat: no-repeat;

	background-position: 0px -73px;

	width: 12px;

}

table.wrapper1 td.td32 {

	height: 12px;

	background-repeat: repeat-x;

	background-position: 0px -55px;

}

table.wrapper1 td.td33 {

	height: 12px;

	background-repeat: repeat-x;

	background-position: 0px -88px;

	width: 12px;

}

