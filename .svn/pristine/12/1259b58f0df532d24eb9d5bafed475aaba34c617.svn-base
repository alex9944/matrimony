/* ICON BUTTONS */





.button-icon {

	/*	text-shadow: 0 1px 1px #432600 !important; */

	text-shadow: none;

	color: #FFF !important;

	display: inline-block;

	height: 17px;

	margin: 0 1em 1em 13px;

	padding: 5px 20px 5px 0;

	position: relative;

	text-decoration: none !important;

	background-image: url(../images/rounded-buttons-right.png);

	background-repeat: no-repeat;

	background-position: right -54px;

	font-family: Arial, Helvetica, sans-serif;

	font-size: 12px;

	font-style: normal;

	line-height: 18px;

	font-weight: bold;

	font-variant: normal;

}

.button-icon span {

	height: 27px;

	left: -13px;

	position: absolute;

	top: 0;

	width: 13px;

	background-image: url(../images/rounded-buttons-left.png);

	background-repeat: no-repeat;

	background-position: left -54px;

}

.button-icon em {

	width: 16px;

	height: 16px;

	float: left;

	margin-right: 5px;

}

.button-icon:hover {

	background: url(../images/rounded-buttons-right.png) no-repeat right -81px;

}

.button-icon:hover span {

	background: url(../images/rounded-buttons-left.png) no-repeat left -81px;

}

.button-icon:hover {

	text-decoration: none;

}

